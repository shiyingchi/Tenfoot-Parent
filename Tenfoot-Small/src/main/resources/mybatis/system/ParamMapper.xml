<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.dao.system.ParamDao">

	<select id="get" resultType="com.project.domain.system.ParamDO">
		select `id`,`param_code`,`param_name`,`param_value`,`createtime`,`remark` from sys_param where id = #{value}
	</select>

	<select id="list" resultType="com.project.domain.system.ParamDO">
		select `id`,`param_code`,`param_name`,`param_value`,`createtime`,`remark` from sys_param

	</select>


</mapper>